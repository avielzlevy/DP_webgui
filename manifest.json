{
    "manifest_version": 3,
    "name": "Datapower Force Old WebGUI",
    "version": "1.0",
    "description": "Redirect /ui/homepage back to root with an optional toggle",
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_title": "Datapower Force Old WebGUI",
        "default_popup": "popup.html"
    }
}